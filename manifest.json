{
  "name": "Notebook Chrome Viewer",
  "version": "0.2.0",
  "description": "A lightweight viewer for Jupyter Notebooks",
  "author": "Andrew O'Brien",
  "permissions": ["declarativeContent"],
  "content_scripts": [
    {
      "matches": ["file://*/*.ipynb"],
      "run_at": "document_end",
      "css": [
        "styles.css",
        "highlight/default.min.css"
      ],
      "js": [
        "view_notebook_bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "mathjax/woff-v2/MathJax_AMS-Regular.woff",
    "mathjax/woff-v2/MathJax_Calligraphic-Bold.woff",
    "mathjax/woff-v2/MathJax_Calligraphic-Regular.woff",
    "mathjax/woff-v2/MathJax_Fraktur-Bold.woff",
    "mathjax/woff-v2/MathJax_Fraktur-Regular.woff",
    "mathjax/woff-v2/MathJax_Main-Bold.woff",
    "mathjax/woff-v2/MathJax_Main-Italic.woff",
    "mathjax/woff-v2/MathJax_Main-Regular.woff",
    "mathjax/woff-v2/MathJax_Math-BoldItalic.woff",
    "mathjax/woff-v2/MathJax_Math-Italic.woff",
    "mathjax/woff-v2/MathJax_Math-Regular.woff",
    "mathjax/woff-v2/MathJax_SansSerif-Bold.woff",
    "mathjax/woff-v2/MathJax_SansSerif-Italic.woff",
    "mathjax/woff-v2/MathJax_SansSerif-Regular.woff",
    "mathjax/woff-v2/MathJax_Script-Regular.woff",
    "mathjax/woff-v2/MathJax_Size1-Regular.woff",
    "mathjax/woff-v2/MathJax_Size2-Regular.woff",
    "mathjax/woff-v2/MathJax_Size3-Regular.woff",
    "mathjax/woff-v2/MathJax_Size4-Regular.woff",
    "mathjax/woff-v2/MathJax_Typewriter-Regular.woff",
    "mathjax/woff-v2/MathJax_Vector-Bold.woff",
    "mathjax/woff-v2/MathJax_Vector-Regular.woff",
    "mathjax/woff-v2/MathJax_Zero.woff"
  ],
  "manifest_version": 2
}